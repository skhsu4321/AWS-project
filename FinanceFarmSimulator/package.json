{
  "name": "financefarmsimulator",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration,e2e,performance,security",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:e2e": "jest --testPathPattern=e2e --runInBand",
    "test:performance": "jest --testPathPattern=performance --runInBand",
    "test:security": "jest --testPathPattern=security --runInBand",
    "test:accessibility": "jest --testPathPattern=accessibility --runInBand",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance && npm run test:security && npm run test:accessibility",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:final": "node scripts/finalTesting.js",
    "readiness:check": "node scripts/deploymentReadiness.js",
    "readiness:quick": "node scripts/deploymentReadiness.js --skip-optional",
    "build": "expo build",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "deploy:dev": "node scripts/deploy.js --environment development",
    "deploy:preview": "node scripts/deploy.js --environment preview",
    "deploy:production": "node scripts/deploy.js --environment production --submit",
    "update:preview": "node scripts/deploy.js --environment preview --ota-update",
    "update:production": "node scripts/deploy.js --environment production --ota-update",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@reduxjs/toolkit": "^2.9.0",
    "@shopify/react-native-skia": "2.2.12",
    "babel-preset-expo": "^54.0.3",
    "expo": "~54.0.10",
    "expo-camera": "~17.0.8",
    "expo-crypto": "^15.0.7",
    "expo-file-system": "~19.0.15",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-router": "~4.0.9",
    "expo-speech": "~14.0.7",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-pdf-lib": "^1.0.0",
    "react-native-reanimated": "^4.1.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vision-camera": "^4.7.2",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "react-redux": "^9.2.0",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "audit-ci": "^7.1.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.4.4",
    "typescript": "~5.9.2"
  },
  "private": true
}
